@model  CouponBuddy.Web.Models.AddUserToRoleModel

@{
    ViewData["Title"] = "AddUserToRole";
}

<h2>AddUserToRole</h2>


<form class="form-group" action="AddUserToRole" method="post">
    <div class="form-group">
        <label>Username</label>
        <input type="text" class="form-control" asp-for="Username" />
    </div>
    <div class="form-group">
        <label>Role</label>
        <input type="text" class="form-control" asp-for="Role" />
    </div>
    <div class="form-group">
        <input type="submit" class="form-control" value="Submit" />
    </div>
</form>