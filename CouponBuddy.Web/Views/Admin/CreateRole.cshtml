@model  CouponBuddy.Web.Models.CreateRoleViewModel

@{
    ViewData["Title"] = "CreateRole";
}

<h2>Create Role</h2>

<form asp-action="CreateRole" method="post" class="form-group">
    <div class="form-group">
        <label class="form-control">Role Name</label>
        <input type="text" class="form-control" asp-for="RoleName" />
    </div>
    <div class="form-group">
        <input type="submit" class="form-control" />
    </div>
</form>